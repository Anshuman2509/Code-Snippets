<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<context:annotation-config/>
	
	<bean id="aa" class="com.bmw.autowiredAnnotation.Car">
	 <property name="id" value="66"></property>
	 <property name="name" value="Audi"></property>
	</bean>
	
	<bean id="bb" class="com.bmw.autowiredAnnotation.Car">
	 <property name="id" value="67"></property>
	 <property name="name" value="Jaguar"></property>
	</bean>
	
	<bean id="cc" class="com.bmw.autowiredAnnotation.Employee">
	 <property name="id" value="1001"></property>
	 <property name="name" value="Ethan Hunt"></property>
	</bean>
	
</beans>